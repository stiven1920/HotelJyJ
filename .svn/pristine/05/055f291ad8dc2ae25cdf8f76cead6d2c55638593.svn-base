package modelo;

/**
 *
 * @author Jopcion
 */
public class Multa {
    
     //creamos los atributos necesarios para la clase Multa haciendo el debido mapeo
    private int idMulta;
    private String estado;
    private int confirmaReservaId;//en la base de datos tiene "recerva" es "reserva"
    private int valor;
    //creamos el constructor
    
    public Multa(){}
    
    public Multa(int idMulta, String estado, int confirmaReservaId) {
        this.idMulta = idMulta;
        this.estado = estado;
        this.confirmaReservaId = confirmaReservaId;
    }
    //luego sus respectivos setters y getters
    public int getIdMulta() {
        return idMulta;
    }

    public void setIdMulta(int idMulta) {
        this.idMulta = idMulta;
    }

    public String getEstado() {
        return estado;
    }

    public void setEstado(String estado) {
        this.estado = estado;
    }

    public int getConfirmaReservaId() {
        return confirmaReservaId;
    }

    public void setConfirmaReservaId(int confirmaReservaId) {
        this.confirmaReservaId = confirmaReservaId;
    }

    public int getValor() {
        return valor;
    }

    public void setValor(int valor) {
        this.valor = valor;
    }
    
}
